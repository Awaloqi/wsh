{
  "name": "wm-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "api:fetch": "curl https://washmix.evrone.app/openapi/ -o schema.yaml",
    "api:generate": "openapi-generator generate -g typescript-fetch -i schema.yaml -o src/api -p useSingleRequestParameter=false,typescriptThreePlus=true",
    "api:validate": "npx @apidevtools/swagger-cli validate schema.yaml",
    "build": "./build_and_rename.sh",
    "build:front": "craco build",
    "build:static": "react-snap",
    "build:st": "react-scripts build",
    "build:storybook": "build-storybook -s public -o build/storybook --quiet",
    "generate:icons": "npx @svgr/cli --typescript --icon -d src/icons src/icons/svg",
    "lint:css": "stylelint 'src/**/*.?(s)css'",
    "lint:css:fix": "stylelint 'src/**/*.?(s)css' --fix",
    "lint:css:stats": "stylelint 'src/**/*.?(s)css' --custom-formatter=node_modules/stylelint-formatter-stats",
    "lint:js": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:js:stats": "eslint src --ext .js,.jsx,.ts,.tsx --format node_modules/eslint-stats/byErrorAndWarning.js",
    "lints": "npm run lint:js && npm run lint:css",
    "lints:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "serve": "npx http-server build -P https://washmix.evrone.app -p 3000",
    "sort": "npx sort-package-json",
    "start": "craco start",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "test": "craco test",
    "tsc:check": "tsc --noEmit",
    "update:cssdb": "npx browserslist@latest --update-db",
    "update:deps": "npx npm-check --no-emoji --update"
  },
  "dependencies": {
    "@craco/craco": "^5.8.0",
    "@googlemaps/js-api-loader": "^1.11.2",
    "@kunukn/react-collapse": "^2.2.9",
    "@sentry/react": "^5.29.2",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/addons": "^6.1.2",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.2",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@szhsin/react-menu": "^1.1.0",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/classnames": "^2.2.11",
    "@types/googlemaps": "^3.40.3",
    "@types/react": "^16.9.53",
    "@types/react-dates": "^17.1.14",
    "@types/react-dom": "^16.9.8",
    "@types/react-geosuggest": "^2.7.10",
    "@types/react-helmet": "^6.1.0",
    "@types/react-modal": "^3.10.6",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/store": "^2.0.2",
    "antd": "^4.19.5",
    "body-scroll-lock": "^3.1.5",
    "bootstrap": "^5.1.3",
    "classnames": "^2.2.6",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "final-form": "^4.20.1",
    "final-form-persist": "^1.0.4",
    "moment": "^2.29.1",
    "parse-google-place": "^1.2.0",
    "prettier": "^2.2.0",
    "query-string": "^6.13.7",
    "react": "^16.8.2",
    "react-dates": "^18.4.1",
    "react-dom": "^16.8.2",
    "react-dropdown": "^1.9.0",
    "react-final-form": "^6.5.2",
    "react-geosuggest": "^2.14.0",
    "react-helmet": "^6.1.0",
    "react-input-mask": "^2.0.4",
    "react-modal": "^3.12.1",
    "react-query": "^2.26.4",
    "react-query-devtools": "^2.6.3",
    "react-responsive": "^8.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-snap": "https://github.com/sairus2k/react-snap#63cbb348d9c06dbd35302c3a602e739205b68126",
    "react-toastify": "^6.1.0",
    "sass": "^1.29.0",
    "store": "^2.0.12",
    "tiny-cookie": "^2.3.2",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "eslint-stats": "^1.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "stylelint": "^13.8.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-formatter-stats": "^0.4.0"
  },
  "engines": {
    "node": "^12.18.0",
    "npm": "^6.14.0"
  },
  "proxy": "http://localhost:3000",
  "reactSnap": {
    "include": [
      "/"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "proxy": "/api",
    "proxyServer": "https://washmix.evrone.app",
    "puppeteerExecutablePath": "/usr/bin/chromium-browser",
    "puppeteer": {
      "waitUntil": "networkidle0"
    }
  },
  "sideEffects": []
}
